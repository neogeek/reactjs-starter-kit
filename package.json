{
  "dependencies": {
    "babel-cli": "6.23.0",
    "doxdox": "2.0.2",
    "ejs": "2.5.6",
    "express": "4.14.1",
    "node-sass": "4.5.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-router": "3.0.2",
    "spire-of-babel": "1.1.22"
  },
  "devDependencies": {
    "@neogeek/eslint-config-standards": "1.13.0",
    "eslint": "3.15.0"
  },
  "scripts": {
    "start": "npm run build:src && node dist/server.js",
    "serve": "babel-node server.js --presets es2015,react",
    "lint": "eslint 'src/**/*.jsx'",
    "build": "npm run build:css && npm run build:js",
    "build:css": "node-sass ./src/css/ --output ./static/css/",
    "watch:css": "node-sass ./src/css/ --watch ./src/css/ --output ./static/css/",
    "build:js": "spire-of-babel ./src/js/index.jsx --bundle --minify --sourcemap --output ./static/js/build.min.js",
    "watch:js": "spire-of-babel ./src/js/index.jsx --bundle --minify --sourcemap --watch './src/js/**/*.jsx' --output ./static/js/build.min.js",
    "build:src": "babel src -d dist --presets es2015,react"
  }
}
